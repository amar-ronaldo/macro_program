{
  "name": "macro-program",
  "productName": "Macro Program",
  "version": "1.0.0",
  "description": "Macro Program untuk buka excel dan input adp",
  "main": "main.js",
  "bin": "cli.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --debug",
    "build": "electron-packager . app --platform win32 --arch x64 --out dist/",
    "setup": "electron-installer-windows --src dist/app-win32-x64/ --dest dist/installers/",
    "rebuild": "npm rebuild --runtime=electron --target=8.0.0 --disturl=https://atom.io/download/atom-shell --abi=76",
    "electron-rebuild": "./node_modules/.bin/electron-rebuild.cmd"
  },
  "repository": "https://github.com/amar-ronaldo/macro_program",
  "author": "8 Code",
  "devDependencies": {
    "@octokit/rest": "^16.43.1",
    "@types/jquery": "^3.3.32",
    "chai": "^3.4.1",
    "chai-as-promised": "^6.0.0",
    "check-for-leaks": "^1.2.1",
    "devtron": "^1.4.0",
    "electron": "^8.0.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^12.1.0",
    "electron-rebuild": "^1.10.0",
    "electron-winstaller": "^2.2.0",
    "husky": "^0.14.3",
    "mocha": "^5.2.0",
    "node-key-sender": "^1.0.11",
    "npm-run-all": "^4.0.2",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "signcode": "^0.5.0",
    "standard": "^8.2.0",
    "electron-installer-windows": "*",
    "electron-prebuilt": "*"
  },
  "dependencies": {
    "active-window": "^0.1.0",
    "dotenv": "^8.2.0",
    "electron-log": "^2.2.14",
    "electron-settings": "^3.0.7",
    "electron-shortcut-normalizer": "^1.0.0",
    "electron-store": "^5.1.0",
    "glob": "^7.1.6",
    "highlight.js": "^9.18.1",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "mysql": "^2.18.1",
    "node-process-windows": "^0.0.2",
    "robotjs": "^0.6.0",
    "update-electron-app": "^1.5.0",
    "xlsx-populate": "^1.20.1"
  },
  "standard": {
    "env": {
      "mocha": true
    }
  }
}
